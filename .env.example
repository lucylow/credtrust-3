# .env.example - Web3Telegram demo
# Wallet/private key used by the sender (DO NOT CHECK IN)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# RPC provider (optional)
RPC_PROVIDER_URL=https://rpc.sepolia.org

# iExec options
IEXEC_IPFS_UPLOAD=https://ipfs-upload.iex.ec
IEXEC_SMS_URL=https://sms.labs.iex.ec
IEXEC_APP_ADDRESS=0xYourAppAddressHere

# Dev mode (dry run, no external calls)
DEV_MODE=true

# TEE / Attestation config (DEV only: for local enclave simulator)
ENCLAVE_PRIVATE_KEY=0xYOUR_TEST_ENCLAVE_PRIVATE_KEY_HERE
ENCLAVE_TYPE=SGX   # or TDX
ATTESTATION_EXPIRY_SECONDS=86400

# RPC & contracts (for on-chain submit)
RPC_URL=https://rpc.sepolia.org
ATTESTATION_REGISTRY_ADDRESS=
